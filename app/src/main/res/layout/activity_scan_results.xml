<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.scanResults.ScanResultsActivity">

    <TableLayout
        android:id="@+id/table"
        android:layout_width="355dp"
        android:layout_height="78dp"
        android:layout_marginBottom="89dp"
        android:layout_marginTop="65dp"
        android:background="@drawable/border"
        android:backgroundTint="?android:attr/textColorHighlight"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="?attr/colorAccent">

            <TextView
                android:id="@+id/algTextView"
                android:layout_width="118dp"
                android:layout_height="wrap_content"
                android:text="Algorithm"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/succTextView"
                android:layout_width="118dp"
                android:layout_height="match_parent"
                android:text="Success (%)"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/totalScansTextView"
                android:layout_width="118dp"
                android:layout_height="match_parent"
                android:text="Scans"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textSize="20sp"
                android:textStyle="bold" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/wifiTextView"
                android:layout_width="118dp"
                android:layout_height="wrap_content"
                android:text="WIFI_RSS"
                android:textAlignment="center"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/wifiSuccTV"
                android:layout_width="118dp"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/wifiScansTV"
                android:layout_width="118dp"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textSize="20sp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/magnTextView"
                android:layout_width="118dp"
                android:layout_height="wrap_content"
                android:text="MAGN_RSS"
                android:textAlignment="center"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/magnSuccTV"
                android:layout_width="118dp"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/magnScansTV"
                android:layout_width="118dp"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textSize="20sp" />

        </TableRow>
    </TableLayout>
</android.support.constraint.ConstraintLayout>